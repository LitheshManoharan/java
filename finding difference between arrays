public class Main {
    public static void main(String[] args) {
        int[] array1 = {1,2};
        int[] array2 = {2,3};
        int[] result = new int[array1.length];

        for (int i = 0; i < array1.length; i++) {
            result[i] = array1[i] - array2[i];
        }
        for (int i = 0; i < result.length; i++) {
            System.out.print(result[i] + " ");
        }
    }
}
