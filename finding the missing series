import java.util.*;
public class Main {
    public static void main(String[] args) {
        int[] a = {2,4,6,10,12};
        int n = 5;
        int d1 = a[1] - a[0];
        int d2 = a[2] - a[1];
        int t = Math.min(d1, d2);
        for (int i = 0;i < n - 1; i++){
            if (a[i] + t != a[i + 1])
            System.out.println(a[i] + t);
        }
    }
}
